{{> header/top-header LoggedInUser = LoggedInUser}}
<div class="wrapper-content bg-gray">
	<div class="gtc-container">
		<div class="row">
			<div class="col-sm-2">
				{{> vendor/vendor-side-navbar}}
			</div>
			<div class="col-sm-10">
				<div class="ibox">
					<div class="ibox-content">
						<div class="ibox-title">
							<h2 class="font-weigh-normal">Track Your Order</h2>
						</div>
						<div class="content-body px-3">
							<div class="row py-4">
								<div class="col-2">
									<img class="img-fluid" width="100px" height="auto" src="{{item.Product.ProductMedia.[0].url}}" title="{{item.Product.product_name}}">
								</div>
								<div class="col-8">
									<p class="font-weigh-bold text-blue">{{item.Product.product_name}}</p>
								</div>
							</div>
							<div class="row">
								<div class="col">
									<small class="text-muted font-weight-light m-b-sm float-right">{{item.OrdersNew.shippingAddress1.address_line1}} {{item.OrdersNew.shippingAddress1.city}}, {{item.OrdersNew.shippingAddress1.State.name}}, {{item.OrdersNew.shippingAddress1.Country.name}}</small>
								</div>
							</div>
							<div class="progress">
								<div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 75%"></div>
							</div>
							<div class="row py-3">
								<div class="col-4">
									<p class="font-weigh-bold text-black m-b-none">
										<strong>Ordered on 10 Dec</strong>
									</p>
								</div>
								<div class="col-4">
									<p class="font-weigh-bold text-black m-b-none">
										<strong>Shipped on 11 Dec</strong>
									</p>
								</div>
								<div class="col-4">
									<p class="font-weigh-bold text-black m-b-none float-right">Delivered on 13 Dec</p>
								</div>
							</div>
							<div class="row m-t-md">
								<div class="col-md-6 offset-md-6">
									<div class="card">
										<div class="card-header-xs bg1 text-md">SHIPMENT DETAILS</div>
									<div class="card-body bg-gray">
										<p class="m-b-none">Last update: Wednesday, 13 Dec</p>
									</div>
								</div>
								<div class="col-md-12 m-t">
									<div class="row">
										<div class="col-md-2">
											<p class="font-weigh-normal text-black m-b-none">11:30am</p>
										</div>
										<div class="col-md-10">
											<p class="font-weigh-normal text-black m-b-none">Your package was delivered</p>
												<div class="d-block">
													<small class="font-weight-light d-inline-block m-r-md">Carrier</small>
													<small class="font-weight-light m-l-sm">CIntel.com Courier Canada Inc.</small>
												</div>
												<div class="d-block">
													<small class="font-weight-light d-inline-block">Tracking #</small>
													<small class="font-weight-light m-l-sm">#170014</small>
												</div>
												<div class="d-block">
													<small class="font-weight-normal"><a href="#">View Order Details</a></small>
												</div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
{{> footer/bottom-footer}}