<section class=" jumbotron product_global">
<h5 class="product_global_search">Search the Global Trade Connect Directory by Category </h5>
<div class="container">
	<div class="row">
            {{#each category}}
            <div class="col-md-3" style="max-width:20%">
                <p class="product_global_para_head">{{ convertUpperCase this.name}}</p>
                {{#each ../subCategory}} {{#ifCond this.category_id '==' ../this.id}}
                <p class="product_global_para">{{ this.name}}</p>
                {{/ifCond}} {{/each}}
            </div>
            {{/each}}
        </div>
        </div>
</section>
