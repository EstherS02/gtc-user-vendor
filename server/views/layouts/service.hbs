<section class="services_banner" style="margin-bottom: 0">
    <div class="container clearfix">
        <div class="row">
            <div class="col-md-6 lstb1">
                <img src="../img/banner.jpg" alt="banner" />
            </div>
            <div class="col-md-6">
                <div class="row">
                    <div class="col-md-8 lstb2">
                        <p>Trying to sell your subscription? Our network</p>
                        <p>has over 25,900 monthly users/buyers.</p>
                    </div>
                    <div class="col-md-4">
                        <input type="submit" value="LIST MY PRODUCT" class="shop_checkout_btn" style="padding: 8px 20px;" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="custom-section">
    <div class="marketplace-header-box"></div>
    <div class="container my-custom-container m-t-n-marketplace">
        <div class="row">
            <div class="col-md-6 no-padding">
                <div class="gtc-box">
                    <div class="gtc-box-market-title public-market-bg">
                        <h5>TOP SELLING SERVICE PROVIDERS</h5>
                        <small class="pull-right m-t-xs">SEE ALL <i class="fa fa-caret-right"></i></small>
                    </div>
                    <div class="gtc-box-content">
                        <div class="feed-element">
                            <div class="feed-product">
                                <img src="shopping.jpeg">
                            </div>
                            <div class="product-info">
                                <div class="row m-l-none m-r-none">
                                    <div class="col-md-9 no-padding">
                                        <div class="product-title">
                                            <h5 class="no-margins">WTS - iPhone Pallets</h5>
                                            <div class="row no-margins">
                                                <div class="col-md-4 no-padding">
                                                    <div class="sm-text">Origin</div>
                                                    <div class="sm-text">Category</div>
                                                    <div class="sm-text">Seller</div>
                                                </div>
                                                <div class="col-md-8 no-padding">
                                                    <div class="sm-text">Canada</div>
                                                    <div class="sm-text">Electronics > Phones</div>
                                                    <div class="sm-text">
                                                        <a href="#">(Login for Seller Info)</a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3 no-padding">
                                        <div class="sm-text pull-right">MOQ: 45</div>
                                        <div class="sm-text pull-right" style="position: absolute; right: 0px; bottom: 15px;">Seller Rating</div>
                                        <div class="sm-text pull-right absolute-b">{{ starCount 4}}</div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6 no-padding">
                <div class="gtc-box">
                    <div class="gtc-box-market-title featured-product-bg">
                        <h5>FEATURED SERVICES</h5>
                        <small class="pull-right m-t-xs">SEE ALL <i class="fa fa-caret-right"></i></small>
                    </div>
                    <div class="gtc-box-content">
                        {{#each featuredService}}
                        <div class="feed-element">
                            <div class="feed-product">
                                <img src="{{this.url}}">
                            </div>
                            <div class="product-info">
                                <div class="row m-l-none m-r-none">
                                    <div class="col-md-9 no-padding">
                                        <div class="product-title">
                                            <h5 class="no-margins">{{ Titlecase this.product_name}}</h5>
                                            <div class="row no-margins">
                                                <div class="col-md-4 no-padding">
                                                    <div class="sm-text">Origin</div>
                                                    <div class="sm-text">Category</div>
                                                    <div class="sm-text">Seller</div>
                                                </div>
                                                <div class="col-md-8 no-padding">
                                                    <div class="sm-text">{{this.origin}}</div>
                                                    <div class="sm-text">{{this.category}} > {{this.sub_category}}</div>
                                                    <div class="sm-text">
                                                        <a href="#">(Login for Seller Info)</a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3 no-padding">
                                        <div class="sm-text pull-right" style="position: absolute; right: 0px; bottom: 15px;">Seller Rating</div>
                                        <div class="sm-text pull-right absolute-b">{{ starCount rating}}</div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="box5" style="margin-bottom:0">
    <div class="container clearfix">
        <div class="row">

            <div class="col-md-12 pmp2">
                <h6 class="float-left">SERVICES MARKETPLACE</h6>
                <p class="float-right see_all">
                    <a href="#">SEE ALL</a>
                    <i class="fa fa-caret-right"></i>
                </p>
            </div>
        </div>
    </div>
</section>
<section class="box4" style="margin-bottom:20px;">
    <div class="container clearfix">
        <div class="row">
            <div class="col-md-6 ">
                <p class="see_all" style="margin: 10px;">Showing most recent 20 product of {{this.count}} products listed.</p>
            </div>
            <div class="col-md-6">
                <p class="see_all float-right" style="margin: 10px;">
                    <i class="fas fa-star" style="color: #C80527;"></i> POST AN AD IN THIS CATEGORY
                    <i class="fa fa-caret-right"></i>
                </p>
            </div>
        </div>
        <div class="row">
            {{#each serviceProduct}}
            <div class="col-md-3 common_top">
                <div class="row">
                    <div class="col-md-3">
                        <img src="{{this.url}}" style="width: 30px; height: 70px;">
                    </div>
                    <div class="col-md-9">
                        <div class="row common_padding">
                            <div class="col-md-8">
                                <p class="wts_header">
                                    <a href="#">{{ Titlecase this.product_name }}</a>
                                </p>
                            </div>
                            <div class="col-md-4">
                                <p>${{this.price}}</p>
                            </div>
                        </div>
                        <div class="row common_padding">
                            <div class="col-md-3">
                                <p>Origin</p>
                            </div>
                            <div class="col-md-5">
                                <p>{{this.origin}}</p>
                            </div>
                            <div class="col-md-4">
                                <p>&nbsp;</p>
                            </div>
                        </div>
                        <div class="row common_padding">
                            <div class="col-md-3">
                                <p>Category</p>
                            </div>
                            <div class="col-md-5">
                                <p>{{this.category}} > {{this.sub_category}}</p>
                            </div>
                            <div class="col-md-4">
                                <p>Seller Rating</p>
                            </div>
                        </div>
                        <div class="row common_padding">
                            <div class="col-md-3">
                                <p>Seller</p>
                            </div>
                            <div class="col-md-5">
                                <p>(
                                    <b>Login for Seller Info</b>)</p>
                            </div>
                            <div class="col-md-4">
                                <p>  {{ starCount this.rating}} </p> 
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</section>