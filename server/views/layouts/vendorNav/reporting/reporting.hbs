<script type="text/javascript">

	$(function () {
		$("#rep_from, #rep_to,#com_from,#com_to").datepicker();
	});

	function onSearch() {
		var from = $('#rep_from').val();
		var to = $('#rep_to').val();
		var Cfrom = $('#com_from').val();
		var Cto = $('#com_to').val();
		var Compare = $('#compare').val();
		location.href = "/reporting?lhs_from=" + from + "&lhs_to=" + to + "&rhs_from=" + Cfrom + "&rhs_to=" + Cto + "&compare=" + Compare;
	}

	$(document).ready(function () {
		//get the line chart canvas
		var ctx = $("#line-chartcanvas");

		var lhsData = [],
			rhsData = [];

		var from = $('#rep_from').val(),
			to = $('#rep_to').val(),
			Cfrom = $('#com_from').val(),
			Cto = $('#com_to').val(),
			Compare = $('#compare').val();

		var dates = [from, to, Cfrom, Cto];
		dates.sort();

		var start = dates[0],
			end = new Date(dates[dates.length - 1]),
			currentDate = new Date(start),
			between = [];

		while (currentDate <= end) {
			between.push($.datepicker.formatDate('M dd', new Date(currentDate)));
			currentDate.setDate(currentDate.getDate() + 1);
		}


		$.ajax({
			type: "GET",
			url: "/api/reports/revenue-changes?lhs_from=" + from + "&lhs_to=" + to + "&rhs_from=" + Cfrom + "&rhs_to=" + Cto + "&compare=" + Compare,
			success: function (data) {
				between.forEach(function (aDate) {
					var objA = _.find(data.past_range, function (pastDate) {
						var pastDateVal = $.datepicker.formatDate('M dd', new Date(pastDate.item_created_on));
						return aDate.toString() == pastDateVal.toString();
					})
					if (objA)
						lhsData.push(parseInt(objA.amount));
					else
						lhsData.push(null);

					var objB = _.find(data.current_range, function (currData) {
						var currDateVal = $.datepicker.formatDate('M dd', new Date(currData.item_created_on));
						return aDate.toString() == currDateVal.toString();
					})
					if (objB)
						rhsData.push(parseInt(objB.amount));
					else
						rhsData.push(null);
				});
				//line chart data
				var data = {
					labels: between,
					backgroundColor: "lightgray",
					datasets: [{
						label: $.datepicker.formatDate('M dd, yy', new Date(from)) + " - " + $.datepicker.formatDate('M dd, yy', new Date(to)),
						data: lhsData,
						backgroundColor: "gray",
						borderColor: "lightgray",
						fill: false,
						lineTension: 0,
						radius: 5
					}, {
						label: $.datepicker.formatDate('M dd, yy', new Date(Cfrom)) + " - " + $.datepicker.formatDate('M dd, yy', new Date(Cto)),
						data: rhsData,
						backgroundColor: "green",
						borderColor: "lightgreen",
						fill: false,
						lineTension: 0,
						radius: 5
					}]
				};

				//options
				var options = {
					responsive: true,
					title: {
						display: true,
						position: "top",
						text: "",
						fontSize: 16,
						fontColor: "#111"
					},
					legend: {
						display: true,
						position: "bottom",
						labels: {
							fontColor: "#333",
							fontSize: 16
						}
					},
					spanGaps: true
				};

				//create Chart class object
				var chart = new Chart(ctx, {
					type: "line",
					data: data,
					options: options
				});
			}
		});

		function calHoursAgo(datetime) {
			var now = new Date();
			var createdAt = new Date(datetime);
			var sec_num = (now - createdAt) / 1000;
			var days = Math.floor(sec_num / (3600 * 24));
			var hours = Math.floor((sec_num - (days * (3600 * 24))) / 3600);
			var minutes = Math.floor((sec_num - (days * (3600 * 24)) - (hours * 3600)) / 60);
			var seconds = Math.floor(sec_num - (days * (3600 * 24)) - (hours * 3600) - (minutes * 60));

			if (hours < 10) {
				hours = "0" + hours;
			}
			if (minutes < 10) {
				minutes = "0" + minutes;
			}
			if (seconds < 10) {
				seconds = "0" + seconds;
			}

			return days + ' days ' + hours + ' hrs ' + minutes + ' min ago';
		}

		function getStatus(statusCode) {
			var status = {
				1: "ACTIVE",
				2: "INACTIVE",
				3: "DELETED",
				4: "OPEN",
				5: "HOLD",
				6: "CLOSED"
			}
			return status[statusCode] ? status[statusCode] : "OPEN";
		}

		function getTopSellingCities() {
			$.ajax({
				type: "GET",
				url: "/api/reports/top-selling-cities",
				success: function (data) {
					data.forEach(function (aObj) {
						var ulTopCities = $("#topCities");
						var tag = `<div class="user-details" >
									<div class="col-md-3 latest-tickets">
										<img src="../../img/img_avatar.png" class="avatar-img">
