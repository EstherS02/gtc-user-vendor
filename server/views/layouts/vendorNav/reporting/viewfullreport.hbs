{{> header/top-header LoggedInUser = LoggedInUser}}{{> vendor/vendor-menu}}
{{#ifCond selectedPage "==" 'revenue'}}
	<div class="wrapper-content bg-gray">
		<div class="gtc-container">
			<div class="row">
				<div class="col-sm-2">
					{{> vendor/vendor-side-navbar}}
				</div>
				<div class="col-sm-10">
					<div class="ibox">
						<div class="ibox-content">
							<div class="content-body">
								<div class="row">
									<div class="col-md-12">
										<h5 class="content-header-title">Ad & Featured Listings Revenue</h5>
										<p class="content-txt">Here you'll be able to generate reports on the ad revenue GTC has generated for both Custom Ads and Featured Lisitings.
											Search by ad type, date range or search by name. You can also export a CSV for your records or two import into
											another accounting program.</p>
										<span class="pull-right" style="font-size:12px">
										</span>
									</div>
								</div>
								<div class="row">
									<div class="col-md-12">
										<table class="table order-table">
											<thead>
												<tr>
													<th scope="col">Ad/Listing Name</th>
													<th scope="col">type</th>
													<th scope="col">Position</th>
													<th scope="col">Start Date</th>
													<th scope="col">End Date</th>
													<th scope="col">Impressions </th>
													<th scope="col">Clicks</th>
													<th scope="col">CTR</th>
													<th scope="col">Cost to Vendor</th>
												</tr>
											</thead>
											{{#if vendorRevenue.rows}}
												{{#each vendorRevenue.rows}}
													<tbody>
														<td>{{this.product_name}}</td>
														<td>{{this.type}}</td>
														<td>{{this.position}}</td>
														<td>{{this.start_date}}</td>
														<td>{{this.end_date}}</td>
														{{#if this.impression}}
															<td>{{this.impression}}</td>
														{{else}}
															<td>0</td>
														{{/if}}
														{{#if this.clicks}}
															<td>{{this.clicks}}</td>
														{{else}}
															<td>0</td>
														{{/if}}
														<td>-</td>
														<td>{{this.Payment.amount}}</td>
													</tbody>
												{{/each}}
											{{/if}}
										</table>
										{{#ifCond vendorRevenue.count '==' 0 }}
											<p style="text-align: center;">No Results!</p>
										{{/ifCond}}
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
{{/ifCond}}
