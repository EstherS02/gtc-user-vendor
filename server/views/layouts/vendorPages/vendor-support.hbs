{{> header/top-header LoggedInUser = LoggedInUser}} {{> vendor/vendor-header VendorDetail = VendorDetail}}
<section class="vendor-product bg-gray">
	<div class="gtc-container">
		<div class="row">
			<div class="col-md-2">
				{{> vendor/vendor-left VendorDetail = VendorDetail}}
			</div>
			<div class="col-md-10">
				<div class="header-lg ">
					<h2 class="header-lg-text">SUPPORT </h2>
				</div>
				<div class="row">
					<div class="col-md-12">
						<div class="ibox">
							<div class="row">
								<div class="col-md-6" style="background-color:#fff;padding: 30px;">
									<div class="col-md-12 no-padding ">
										<p id="shipping-policy" class="terms_head">SHIPPING POLICY </p>
										{{#if Support.shipping_policy}}
											<p class="terms_para">{{{Support.shipping_policy}}}</p>
										{{else}}
											<p class="terms_para">Not yet updated by vendor</p>
										{{/if}}
									</div>
									<hr>
									<div class="col-md-12 no-padding">
										<p id="return-policy" class="terms_head">RETURN POLICY</p>
										{{#if Support.return_policy}}
											<p class="terms_para">{{{Support.return_policy}}}</p>
										{{else}}
											<p class="terms_para">Not yet updated by vendor</p>
										{{/if}}
									</div>
									<hr>
									<div class="col-md-12 no-padding">
										<p id="terms-of-use" class="terms_head">TERMS OF USE</p>
										{{#if Support.terms_of_use}}
											<p class="terms_para">{{{Support.terms_of_use}}}</p>
										{{else}}
											<p class="terms_para">Not yet updated by vendor</p>
										{{/if}}
									</div>
									<hr>
								</div>
								<div class="col-md-3">
									<div class="dis_side_bar">
										<a href="support/ticket/create-Ticket">
											<p class="terms_topics-para">Submit A Support Ticket</p>
										</a>
										{{!-- <a href="/terms-and-conditions">
											<p class="terms_topics-para">Submit A Return</p>
										</a> --}}
										<a href="vendor/support/{{this.VendorDetail.id}}#shipping-policy">
											<p class="terms_topics-para">Shipping Policy </p>
										</a>
										<a href="vendor/support/{{this.VendorDetail.id}}#return-policy">
											<p class="terms_topics-para">Return Policy </p>
										</a>
										<a href="vendor/support/{{this.VendorDetail.id}}#terms-of-use">
											<p class="terms_topics-para">Terms of Use </p>
										</a>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>
{{> footer/bottom-footer}}