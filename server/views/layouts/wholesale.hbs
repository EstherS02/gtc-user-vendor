{{> header/top-header LoggedInUser = LoggedInUser}}
<script>
  $(document).ready(function () {
      $('#marketplace_type').on('change', function () {
        $('#searchSubmit').prop('disabled', !$(this).val());
      }).trigger('change');
      $('#category').on('change', function () {
        $('#searchSubmit').prop('disabled', !$(this).val());
      }).trigger('change');
      $('#location').on('change', function () {
        $('#searchSubmit').prop('disabled', !$(this).val());
      }).trigger('change');
      $("#searchForm").submit(function (e) {
        e.preventDefault();
        let formInput = $("#searchForm :input").filter(function (index, element) {
          return $(element).val() != '';
        }).serialize();
        window.location.href = 'wholesale/search?' + formInput;
      });

      var userInfo = {{{ DisplayJSON LoggedInUser }}}

      $("a[title='seller']").click(function () {
        console.log("clicked");
        if (userInfo.email) {
        } else {
          $('#myModal').modal('show');
        }
      });
  });
</script>

<script src="js/login.js"></script>

<section class="lifestyle_banner" style="margin-bottom: 0; background-color: #5D5646;">
	<div class="container-fluid full_view_pad_eighty clearfix">
    <div class="row">
      <div class="col-md-6 col-12 lstb1">
        <img src="../img/direc_banner.png" alt="banner" />
      </div>
      <div class="col-md-6 col-12">
        <div class="row">
         <div class="col-md-12 col-12 lstb2 text-center">
          <form id="searchForm">
            <p>Our Wholesale Marketplace has over</p>
            <p>12,000 Wholesalers & 60,000 Products</p>
            <p class="adv_search">Advance Search</p>
            <div class="row">
              <div class="col-md-4 col-12 common_padding1">
                <select class="geo_textbox" name="marketplace_type" id="marketplace_type">
                  <option disabled selected>Type</option>
                  {{#each type}}
                  <option value="{{this.id}}">{{ this.name}}</option>
                  {{/each}}
                </select>
              </div>
              <div class="col-md-4 col-12 common_padding1">
               <select class="geo_textbox" name="category" id="category">
                <option disabled selected>Category</option>
                {{#each categories}}
                <option value="{{this.id}}">{{ this.name}}</option>
                {{/each}}
              </select>
            </div>
            <div class="col-md-4 col-12 common_padding1">
             <select class="geo_textbox" name="location" id="location">
              <option disabled selected>Location</option>
              {{#each country}}
              <option value="{{this.id}}">{{ this.name}}</option>
              {{/each}}
            </select>
          </div>
        </div>
        <input type="submit" id="searchSubmit" value="SEARCH WHOLESALE" class="shop_checkout_btn" style="padding: 8px 20px;" />
      </form>
    </div>
  </div>
</div>
</div>
</div>
</section>

