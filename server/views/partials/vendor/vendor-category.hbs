    {{!-- <p><b>FILTER BY CATEGORY</b></p>
    <p class="cat_bg_title">ALL CATEGORIES ({{categories.count}})</p> --}}
    <div class="list-group panel ">
     <ul class="list-group">
      {{#each categoriesWithCount.rows}}
      <li class="list-group-item">
        <a data-toggle="collapse" href="#collapseCategory{{this.id}}" role="button" aria-expanded="true" aria-controls="collapseCategory{{this.id}}">
          <span>{{this.name}} ({{this.product_count}})</span>
        </a>
        <ul class="list-group collapse" id="collapseCategory{{this.id}}">
          {{#each this.SubCategories}}
          <li class="list-group-item">
            <a {{#ifCond ../../selectedSubCategory "==" this.id}} class="text-black" {{/ifCond}} href="../search?category={{../this.id}}&sub_category={{this.id}}&vendor_id={{../../VendorDetail.id}}">
              <span>{{this.name}}</span>
            </a>
            <ul></ul>
          </li>
          {{/each}}
        </ul>
      </li>
      {{/each}}
    </ul>
  </div>
  {{#if marketPlaceTypes}}
  <p style="margin-top: 20px;"><b>FILTER BY TYPE</b></p>
  <p class="cat_bg_title">ALL TYPES ({{marketPlaceTypes.count}})</p>
  <div class="list-group panel ">
    <ul class="list-group">
      {{#each marketPlaceTypes.rows}}
      <li class="list-group-item">
        <a  class="text-black" }} href="../search?&vendor_id={{../VendorDetail.id}}&marketplace={{../marketPlace.WHOLESALE}}&marketplace_type={{this.id}}">
          <span>{{this.code}} - {{this.name}} ({{this.product_count}})</span>
        </a>
      </li>
      {{/each}}
    </ul>
  </div>
  {{/if}}