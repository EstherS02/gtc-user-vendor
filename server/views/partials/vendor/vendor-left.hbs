	<div class="table-responsive">
		<table class="table shoping-cart-table">
			<tbody>
				<tr>
					<td><span class="text-muted">Joined</span></td>
					<td class="text-left"><span><strong>{{formatTime VendorDetail.created_on "MMMM Y"}}</strong></span></td>
				</tr>
				<tr>
					<td><span class="text-muted">Location</span></td>
					<td class="text-left">{{#if VendorDetail.Country.code}}<img src="../gtc-assets/img/flag/{{VendorDetail.Country.code}}.png" class="img-fluid" alt="Canada" title="Canada" /><span class="m-l-xs" style="vertical-align: middle;">{{/if}}<strong>{{VendorDetail.Country.name}}</strong></span></td>
				</tr>
				<tr>
					<td><span class="text-muted">User Rating</span></td>
					<td><div class="p-rating">
							{{starCount this.avgRating}}
			            </div>
						{{#ifCond LoggedInUser.Vendor.id '==' VendorDetail.id}}
						<p class="v-rating">
							<a href="/reviews">
								<strong>({{this.ratingCount}} Ratings - See All)</strong>
							</a>
						</p>
						{{/ifCond}}
					</td>
					{{!-- {{#if VendorAvgRating}}
					<td>
						{{#each VendorAvgRating}}
						<div class="p-rating">
							{{starCount this.rating}}
			            </div>
						<p class="v-rating">
							<a href="/reviews">
								<strong>({{decimalFixedOne this.rating}} Ratings - See All)</strong>
							</a>
						</p>
						{{/each}}
					</td>
					{{else}}
					<td>
						<div class="p-rating">
							{{starCount 0}}
			            </div>
			            <p class="v-rating"><a href="/reviews"><strong>(0 Ratings - See All)</strong></a></p>
					</td>
					{{/if}} --}}
				
				</tr>
			</tbody>
		</table>
