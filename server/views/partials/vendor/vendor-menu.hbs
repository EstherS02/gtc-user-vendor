												{{else ifCond LoggedInUser.Vendor.VendorPlans.[0].plan_id '==' vendorPlan.LIFESTYLE_PROVIDER}} Lifestyle Provider 
												{{else ifCond LoggedInUser.Vendor.VendorPlans.[0].plan_id '==' vendorPlan.PUBLIC_SELLER}} Retailer
												{{else ifCond LoggedInUser.Vendor.VendorPlans.[0].plan_id '==' vendorPlan.BULK_BUYER}} Bulk buyer 
												{{else ifCond LoggedInUser.Vendor.VendorPlans.[0].plan_id '==' vendorPlan.WHOLESALER}} Wholesaler
											{{/ifCond}}
											{{#ifCond LoggedInUser.UserPlans.[0].plan_id '==' vendorPlan.BULK_BUYER}} 
												,<br/>
												&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Bulk Buyer
											{{/ifCond}}
										</b>
									{{else}}
										<b>Vendor plan expired 
										{{#ifCond LoggedInUser.UserPlans.[0].plan_id '==' vendorPlan.BULK_BUYER}} 
											,<br/>
											User Plan: Bulk Buyer
										{{/ifCond}}
										</b> 
									{{/if}}
								</a>
							</li> --}}
							<li class="nav-item">
								{{#if LoggedInUser.Vendor.VendorPlans.[0].plan_id}}
								<a href="/upgradeplan" class="nav-link" style="color:#2775c3 !important;">Current Plan</a>
								{{else}}
									<b>Vendor plan expired </b>
								{{/if}}
							</li>
							{{#ifCond LoggedInUser.Vendor.VendorPlans.[0].plan_id '==' vendorPlan.WHOLESALER}}
								<li class="nav-item">
									<a href="listings/wholesale" class="nav-link">Wholesale</a>
								</li>
							{{/ifCond}}
							<li class="nav-item">
								<a href="/gtc-mail/inbox" class="nav-link">Inbox{{LoggedInUser.UserPlans.plan_id}}{{vendorPlan.BULKBUYER}}
									<b class="notification-badge">{{unreadCounts.mailCount}}</b>
								</a>
							</li>
							<li class="nav-item">
								<a href="/messages" class="nav-link">Messages
									<strong><small id="msg-count"></small></strong>
								</a>
							</li>
							<li class="nav-item">
								<a href="/notifications" class="nav-link">Notifications
									<b class="notification-badge">{{unreadCounts.notification}}</b>
								</a>
							</li>
							<li class="nav-item">
								<a href="/promote-store" class="nav-link" style="color:#2775c3 !important;">Promote Your Store</a>
							</li>
							<li class="nav-item">
								<a href="/dashboard-vendor-connect" class="nav-link" style="color:#2775c3 !important;">Build Your Store</a>
							</li>
						</ul>
							{{!-- <span class="support" style="font-family:'Roboto', sans-serif">Support</span> --}}
						<div id="dropdownField" class="dropdown cursor-pointer">
							<a href="/vendor/support/{{this.LoggedInUser.Vendor.id}}" id="dropdownMenuButton" data-toggle="dropdown" aria-expanded="false">
								<span class="support" id="supportHelp" style="font-family:'Roboto', sans-serif">Support</span>
							</a>
							<div class="dropdown-menu dropdown-menu-list" aria-labelledby="dropdownMenuButton">
								<ul>
									<li>
										<a href="https://www.youtube.com/channel/UCR2WynZ2voPqi4aNhPlpK9g">Help</a>
									</li>
								</ul>
							</div>
						</div>
					</div>
				</nav>
			</div>
		</div>
	</div>
{{else}}
	<div class="vendor-body">
		<div class="vendor-menu-wrapper">
			<div class="gtc-container">
				<nav class="navbar navbar-expand-sm vendor-menu">
					<a class="navbar-brand" href="/user/my-profile">
						<img src="{{LoggedInUser.user_pic_url}}" width="60" onError="UserImgError(this)" height="40" class="d-inline-block object-fit"
						 alt="">
						<a href="/user/my-profile">
							<span class="vendor-name">{{LoggedInUser.first_name}}</span>
						</a>
					</a>
					<button class="navbar-toggler collapsed" type="button" data-toggle="collapse" data-target="#vendorNavBar" aria-controls="vendorNavBar"
					 aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>
					<div class="navbar-collapse collapse" id="vendorNavBar">
						<ul class="navbar-nav vendor-nav">
							{{#if LoggedInUser.UserPlans}}
								<li class="nav-item">
									<a href="javascript:;" class="nav-link" style="cursor:default">
										<b>Current Plan:
											{{#ifCond LoggedInUser.UserPlans.[0].plan_id '==' vendorPlan.BULK_BUYER}} Bulk Buyer
											{{/ifCond}}
										</b>
									</a>
								</li>
							{{/if}}
							<li class="nav-item">
								<a href="/gtc-mail/inbox" class="nav-link">Inbox
									<b class="notification-badge">{{unreadCounts.mailCount}}</b>
								</a>
							</li>
							<li class="nav-item">
								<a href="/messages" class="nav-link">Messages
									<strong><small id="msg-count"></small></strong>
								</a>
							</li>
							<li class="nav-item">
								<a href="/notifications" class="nav-link">Notifications
									<b class="notification-badge">{{unreadCounts.notification}}</b>
								</a>
							</li>
						</ul>
						<div id="dropdownField" class="dropdown cursor-pointer">
							<a href="javascript:;" id="dropdownMenuButton" data-toggle="dropdown" aria-expanded="false">
								<span class="support" style="font-family:'Roboto', sans-serif">Support</span>
							</a>
							<div class="dropdown-menu dropdown-menu-list" aria-labelledby="dropdownMenuButton">
								<ul>
									<li>
										<a href="https://www.youtube.com/channel/UCR2WynZ2voPqi4aNhPlpK9g">Help</a>
									</li>
								</ul>
							</div>
						</div>
					</div>
				</nav>
			</div>
		</div>
	</div>
{{/if}}

<script type="text/javascript">
	$(document).ready(function() {
         $.ajax({
            type: 'GET',
            url: '/api/talk/talk-count',
            success: function (data) {
                document.getElementById('msg-count').innerHTML = `<b class="notification-badge"> `+data.length +`</b>`; 
            },
            error: function (error) {
                console.log(error);
            }
        })

		socket.on('connect', () => {
			socket.on('chat:count:receive', function(result) {
				document.getElementById('msg-count').innerHTML = `<b class="notification-badge"> ` + result + `</b>`;
			})
		});

		$('#supportHelp').click(function(e){
			var vendor = {{{ DisplayJSON LoggedInUser.Vendor }}};
			if(vendor != null){
				window.location.href = '/vendor/support/'+vendor.id;
			}
		});
	});
</script>