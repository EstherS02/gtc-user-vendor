<script type="text/javascript">
	function openReportPage() {
		window.location.href = "store/reports";
	}

	function openAccountingPage() {
		window.location.href = "/store/accounting";
	}

	$(document).ready(function() {
		var current = location.pathname;
		if (current.indexOf("reports") !== -1) {
			$('#collapseReporting').addClass('show');
			$('#ReportingLink').addClass('active');
		}
		if (current.indexOf("accounting") !== -1) {
			$('#collapseAccounting').addClass('show');
			$('#AccountingLink').addClass('active');
		}
		$('#StoreNavbar li a').each(function() {
			var $this = $(this);
			if ($this.attr('href').indexOf(current) !== -1) {
				$this.addClass('active');
			}
		});

		$('#UserNavbar li a').each(function() {
			var $this = $(this);
			if ($this.attr('href').indexOf(current) !== -1) {
				$this.addClass('active');
			}
		});

		$('#collapseMarketplaceListing li a').each(function() {
			var $this = $(this);
			if (current.indexOf($this.attr('href')) !== -1) {
				$this.addClass('active');
			}
		});

		$('#collapseGTCMail li a').each(function() {
			var $this = $(this);
			if ($this.attr('class') && $this.attr('class').indexOf('active') !== -1) {
				$('#collapseGTCMail').addClass('show');
			}
		});

		$('#collapseMarketplaceListing li a').each(function() {
			var $this = $(this);
			if ($this.attr('class') && $this.attr('class').indexOf('active') !== -1) {
				$('#collapseMarketplaceListing').addClass('show');
			}
		});

		$('#collapseReporting li a').each(function() {
			var $this = $(this);
			if ($this.attr('class') && $this.attr('class').indexOf('active') !== -1) {
				$('#collapseReporting').addClass('show');
			}
		});

		$('#collapseAccounting li a').each(function() {
			var $this = $(this);
			if ($this.attr('class') && $this.attr('class').indexOf('active') !== -1) {
				$('#collapseAccounting').addClass('show');
			}
		});
	});
</script>

