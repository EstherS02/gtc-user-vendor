<script type="text/javascript">
    function myFunction(){
        window.location.href="/reporting";
    }
    function myAccountFunction(){
        window.location.href="/reporting/accounting";
    }
    $( document ).ready(function() {
         $.ajax({
			type: 'GET',
			url: '/api/mail/unReadMailCount',
			success: function (data) {
                if(data.userMailCount>0){
                   $(".UnreadMessages").html(data.userMailCount);
                }      
			},
			error: function (error) {
                console.log(error);
			}
        })
    });
        
</script>

